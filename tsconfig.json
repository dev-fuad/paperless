{
  "compilerOptions": {
    "baseUrl" : "./",
    "paths": {
      "@components": ["components/index"],
      "@components/*": ["components/*"],
      "@store/*": ["store/*"],
      "@models": ["models/index"],
      "@models/*": ["models/*"],
      "@services/*": ["services/*"],
      "@utils/*": ["utils/*"],
      "@animations/*": ["animations/*"],
    }
  },
  "extends": "expo/tsconfig.base",
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
